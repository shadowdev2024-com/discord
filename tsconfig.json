{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["WebWorker", "ES2020"],
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "types": ["node"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["api"]
}
